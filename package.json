{
  "private": true,
  "name": "vynl-monorepo-root",
  "version": "1.0.0",
  "scripts": {
    "start": "npm --prefix ./vynl run start",
    "android": "npm --prefix ./vynl run android",
    "ios": "npm --prefix ./vynl run ios",
    "web": "npm --prefix ./vynl run web",
    "lint": "npm --prefix ./vynl run lint",
    "reset-project": "npm --prefix ./vynl run reset-project",
    "install": "npm --prefix ./vynl install",
    "test": "npm --prefix ./vynl run test "
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@supabase/supabase-js": "^2.76.1",
    "expo": "^54.0.21",
    "expo-image": "^3.0.10",
    "expo-router": "^6.0.13"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "^19.2.2",
    "@types/react-native": "^0.72.8",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.13"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ]
  }
}
